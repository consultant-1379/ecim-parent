         id="ECIM Base Profile UMLProfiles"
         name="ECIM Base Profile UMLProfiles"
         point="org.eclipse.gmf.runtime.emf.core.Pathmaps">
      <pathmap
            name="ECIM_BASE_PROFILE"
            plugin="com.ericsson.ecim.dsl"
            path="profiles">
      </pathmap>
   </extension-->

   <extension point="org.eclipse.emf.ecore.uri_mapping">
      <mapping source="pathmap://ECIM_BASE_PROFILE/" target="platform:/plugin/com.ericsson.ecim.dsl/profiles/">
      </mapping>
   </extension>
   
