  </extension>
  
      <extension id="ECIM_BASE_PROFILE" name="ECIM_BASE_PROFILE" point="org.eclipse.papyrus.uml.extensionpoints.UMLProfile">
         <profile name="ECIM Base Profile" path="pathmap://ECIM_BASE_PROFILE/ECIM_Base_Profile.profile.uml">
         </profile>
      </extension>
      <extension point="org.eclipse.papyrus.uml.diagram.wizards.templates">
