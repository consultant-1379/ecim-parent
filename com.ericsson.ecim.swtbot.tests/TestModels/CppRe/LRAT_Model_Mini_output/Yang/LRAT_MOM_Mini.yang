			type int32 {
				range "0..999";
			}

			description
			"Mobile Country Code (a part of the PLMN identity) for a
					cell that is served by a neighbor eNB.
";
		}

		leaf "mnc" {
			type int32 {
				range "0..999";
			}

			description
			"Mobile Network Code (a part of the PLMN identity) for a
					cell that is served by a neighbor eNB.
";
